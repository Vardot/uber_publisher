{
  "name": "vardot/uber_publisher",
  "description": "Uper Publisher distribution for bulding enterprise publishing protals.",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Vardot",
      "homepage": "https://www.drupal.org/vardot",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/uber_publisher",
    "source": "http://cgit.drupalcode.org/uber_publisher"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/core-composer-scaffold": "^8.8.0",
    "drupal/drupal-library-installer-plugin": "^0.3",
    "webflo/drupal-finder": "^1.1.0",
    "vardot/varbase": "~8.8.0",
    "drupal/linked_field": "^1.3",
    "drupal/sharerich": "^1.2",
    "drupal/blocktabs": "^1.0-beta3",
    "drupal/social_post_twitter": "^2.0-alpha4",
    "drupal/views_exclude_previous": "^1.0.0-rc1",
    "drupal/fb_instant_articles": "^2.2",
    "drupal/ads_system": "^1.0-alpha1",
    "drupal/simple_amp": "^1.18",
    "drupal/subpathauto": "^1.0",
    "drupal/facets": "^1.4",
    "lullabot/amp": "^1.1",
    "drupal/media_library_edit": "^1.0",
    "drupal/allowed_formats": "^1.2",
    "drupal/varbase_content_planner": "1.x-dev",
    "drupal/uber_publisher_core": "6.x-dev",
    "drupal/uber_publisher_author_profile": "6.x-dev",
    "drupal/uber_publisher_opinion_articles": "6.x-dev",
    "drupal/uber_publisher_ads_system": "6.x-dev",
    "drupal/uber_publisher_breaking_news": "6.x-dev",
    "drupal/uber_publisher_daily_cartoons": "6.x-dev",
    "drupal/uber_publisher_in_pictures": "6.x-dev",
    "drupal/uber_publisher_search": "6.x-dev",
    "drupal/uber_publisher_social_post": "6.x-dev"
  },
  "require-dev": {
    "drupal/core-dev": "^8.8.0",
    "drupal/console": "^1.0",
    "drush/drush": "^9.0",
    "behat/behat": "~3.0",
    "phing/phing": "~2.0",
    "drupal/drupal-extension": "^3.2",
    "emuse/behat-html-formatter": "^0.2.0"
    },
    "config": {
      "bin-dir": "bin/",
      "secure-http": false,
      "optimize-autoloader": true,
      "preferred-install": {
        "drupal/core": "dist"
      }
    },
      "autoload": {
      "psr-4": {
      "Varbase\\composer\\": "src/composer"
      }
    },
    "scripts": {
      "pre-install-cmd": [
        "Varbase\\composer\\ScriptHandler::checkComposerVersion"
      ],
      "pre-update-cmd": [
        "Varbase\\composer\\ScriptHandler::checkComposerVersion"
      ],
      "post-install-cmd": [
        "Varbase\\composer\\ScriptHandler::createRequiredFiles",
        "Varbase\\composer\\ScriptHandler::removeGitDirectories",
        "./bin/phing push"
      ],
      "post-update-cmd": [
        "Varbase\\composer\\ScriptHandler::createRequiredFiles",
        "Varbase\\composer\\ScriptHandler::removeGitDirectories",
        "./bin/phing push"
      ],
      "post-drupal-scaffold-cmd": [
        "Varbase\\composer\\ScriptHandler::postDrupalScaffoldProcedure",
        "UberPublisher\\composer\\UberPublisherScriptHandler::postDrupalScaffoldSubProfileProcedure"
      ]
    },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      }
    },
    "branch-alias": {
      "dev-8.x-6.x": "8.6.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/profiles/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/libraries/slick": [
        "npm-asset/slick-carousel"
      ],
      "docroot/libraries/ace": [
        "npm-asset/ace-builds"
      ],
      "docroot/libraries/masonry": [
        "npm-asset/masonry-layout"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-libraries": {
      "library-directory": "docroot/libraries",
      "libraries": [
        {
          "name": "dropzone",
          "package": "npm-asset/dropzone"
        },
        {
          "name": "blazy",
          "package": "npm-asset/blazy"
        },
        {
          "name": "slick",
          "package": "npm-asset/slick-carousel"
        },
        {
          "name": "ace",
          "package": "npm-asset/ace-builds"
        },
        {
          "name": "chartjs",
          "package": "bower-asset/chartjs"
        },
        {
          "name": "c3",
          "package": "bower-asset/c3"
        },
        {
          "name": "d3",
          "package": "bower-asset/d3"
        },
        {
          "name": "masonry",
          "package": "npm-asset/masonry-layout"
        },
        {
          "name": "imagesloaded",
          "package": "npm-asset/imagesloaded"
        },
        {
          "name": "swagger-ui",
          "package": "swagger-api/swagger-ui"
        }
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
       "drupal/core": "-p2"
    },
    "patches": {
      "drupal/fb_instant_articles": {
        "Issue #2912129: Fix embed YouTube, Twitter & Instagram?":
        "https://www.drupal.org/files/issues/twitter_instagram_table_picture_transformer-2912129-7.patch"
      }
    }
  }
}
