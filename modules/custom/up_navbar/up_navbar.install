<?php

/**
 * @file
 * Install, update and uninstall functions for the menu module.
 */

/**
 * Implements hook_install().
 */
function up_navbar_install() {
  $t = get_t();
  $menu = array(
    'menu_name' => 'add-content',
    'title' => $t('Add Content'),
    'description' => $t('Add Content link'),
  );
  menu_save($menu);
}
