<?php
/**
 * @file
 * uber_publisher_disqus.install
 */

/**
 * Implements hook_disable().
 * 
 */
function uber_publisher_disqus_disable() {
  if (module_exists('disqus')) {
    module_disable(array('disqus'), FALSE);
  }
}

/**
 * Implements hook_uninstall().
 * 
 */
function uber_publisher_disqus_uninstall() {
  drupal_uninstall_modules(array('disqus'), FALSE);
}
