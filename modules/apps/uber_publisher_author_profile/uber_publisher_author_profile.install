<?php

/**
 * Implements hook_install()
 */
function uber_publisher_author_install() {
  // services module settings for avilable content types and categories.
  $node_type = variable_get('service_links_node_types', array());
  $node_type['uber_publisher_author'] = 'uber_publisher_author';
  variable_set('service_links_node_types', $node_type);
}
